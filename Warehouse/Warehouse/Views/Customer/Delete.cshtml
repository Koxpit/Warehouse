@model Warehouse.Models.Customer
@{
    ViewBag.Title = "Удаление заказчика";
}

<div>
    <h1>Удалить заказчика @Model.FIO (@Model.PhoneNumber)?</h1>
    <div>
        <form asp-controller="Customer" asp-action="Delete" method="post">
            <div class="form-group">
                <input type="submit" class="btn btn-default" value="Удалить" />
            </div>
        </form>
    </div>
</div>