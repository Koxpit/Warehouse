@model Warehouse.Models.Cargo
@{
    ViewBag.Title = "Удаление груза";
}

<div>
    <h1>Удалить груз №@Model.Number?</h1>
    <div>
        <form asp-controller="Cargo" asp-action="Delete" method="post">
            <div class="form-group">
                <input type="submit" class="btn btn-default" value="Удалить" />
            </div>
        </form>
    </div>
</div>